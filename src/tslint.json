{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "",
      "kebab-case"
    ],
    "whitespace": false,
    "no-unused-expression": [false, "allow-fast-null-checks"]
  }
}
